{
    "manifest_version": 3,
    "name": "modify",
    "version": "1.0.0.0",
    "description": "支持修改network",
    "background": {
        "service_worker": "background.js"
      },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content_script.js"
            ],
            "run_at": "document_start"
        }
    ],
    "permissions": [
        "contextMenus",
        "tabs",
        "notifications",
        "webRequest",
        "webRequestBlocking",
        "storage"
    ]
}